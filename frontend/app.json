{
  "expo": {
    "name": "Rapid Donor",
    "slug": "Rapid-Donor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/main.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/main.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.priyanshu0ko.RapidDonor",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/main.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.priyanshu0ko.RapidDonor",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/main.jpeg",
          "color": "#d32f2f",
          "sounds": [
            "./assets/notification.wav",
            "./assets/notification.mp3"
          ],
          "defaultChannel": "default",
          "mode": "production"
        }
      ],
      "expo-dev-client"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e4675282-f0eb-4967-a86c-7261e2d0c884"
      }
    },
    "owner": "priyanshu0ko"
  }
}